{
  "$schema": "../gen/schemas/mobile-schema.json",
  "identifier": "mobile-capability",
  "windows": [
    "main"
  ],
  "platforms": [
    "iOS",
    "android"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:image:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "geolocation:default",
    "geolocation:allow-check-permissions",
    "geolocation:allow-request-permissions",
    "geolocation:allow-get-current-position",
    "geolocation:allow-watch-position",
    "store:default",
    "log:default",
    "blec:default",
    "deep-link:default"
  ]
}
